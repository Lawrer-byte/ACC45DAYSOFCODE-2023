#include <iostream>
#include <math.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    string a;
	    cin>>a;
	    
	    int s=0,i;
	    for(i=0; i<n; i++){
	       if(a[i]=='1')
	       s++;
	   }
	   s=s+(120-n);
	   if(s>=90)
	   cout<<"YES"<<endl;
	   else cout<<"NO"<<endl;
	}
	return 0;
}
