# cook your dish here
t=int(input())
for i in range(t) :
    a=list(map(int,input().split()))
    b=list(map(int,input().split()))
    x=0 
    y=0
    for i in range(3) :
        if(a[i]>b[i]) :
            x+=1
        elif(b[i]>a[i]) :
            y+=1
    if(x>y):
        print("A")
    else:
        print("B")
    
    
