#include <iostream>

using namespace std;

int main()
{
    int x,z,t,c;
    cin>>t;
    while(t--)
    {
        cin>>x>>z;
        if(x>z)
        {
            c=x-z;
            cout<<c<<endl;
        }
        else
        cout<<0<<endl;
        
        
        
        
    }

    return 0;
}
