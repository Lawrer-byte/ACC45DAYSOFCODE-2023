#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int  main()
{
    // your code goes here
	double a,b;
	cin>>a>>b;
	char c;
    cin >>c;
    if ( c == '/' )
    cout <<fixed << setprecision(7) << a/b;
    else if ( c == '*' )
        cout <<a*b;
    else if ( c == '+' )
        cout  <<a+b;
    else 
    cout  <<a-b;
    return 0;

}
